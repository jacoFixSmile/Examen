<!DOCTYPE html>
<html lang='en'>
<head>
  <meta charset='UTF-8'>
  <title>Products</title>
</head>
<body>
  <h1>All products</h1>
 <br/><br/>
  <ul>
  <% products.forEach(function(product) { %>
    <li>
      <span><b>ID : </b><%= product.id %></span><br />
      <span><b>Name : </b><%= product.value.name %></span><br />
    
    </li>
      <span></span><br />
    </li>
  <% }) %>
  </ul>
<br/>

</body>
</html>